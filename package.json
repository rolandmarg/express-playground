{
  "name": "express-playground",
  "version": "1.0.0",
  "description": "express, ts, graphql, typeorm playground",
  "main": "server.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romMidnight/express-playground.git"
  },
  "keywords": [
    "express",
    "typescript",
    "graphql",
    "typeorm"
  ],
  "author": "@romMidnight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romMidnight/express-playground/issues"
  },
  "homepage": "https://github.com/romMidnight/express-playground#readme",
  "scripts": {
    "dev": "concurrently 'tsc -w' 'node-dev --respawn dist/server.js' 'yarn generate --watch'",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts}\"",
    "type-check": "tsc --pretty --noEmit",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn type-check && yarn test"
    }
  },
  "lint-staged": {
    "*.@(ts)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "apollo-server-express": "^2.16.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "graphql": "^15.3.0",
    "helmet": "^3.23.3",
    "ms": "^2.1.2",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.3.0",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.6",
    "@graphql-codegen/introspection": "1.17.6",
    "@graphql-codegen/typescript": "1.17.6",
    "@graphql-codegen/typescript-resolvers": "1.17.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^26.0.8",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.0.27",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "node-dev": "^5.1.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}